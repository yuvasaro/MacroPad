int profile = 0;
bool dset = true;

void loop() {
  // put your main code here, to run repeatedly:

  switch(profile)
  {
      case 0:
        if(digitalRead(BUTTON1) == 0){ openVSCode(); Serial.println("Button 1 Pressed - VSCode");}
        if(digitalRead(BUTTON2) == 0){ openArduino(); Serial.println("Button 2 Pressedc - Arduino");}
        if(digitalRead(BUTTON3) == 0){ openSpotify(); Serial.println("Button 3 Pressed - Spotify");}
        if(digitalRead(BUTTON4) == 0){ openNotion(); Serial.println("Button 4 Pressed - Notion");}

        delay(200);
        if(!dset)
        {
          display.clearDisplay();
          //im1d1 code
          display.drawBitmap(0, 0, im1d2_bmp, 128, 64, WHITE);
          display.display();
          dset = true;
        }
        break;
      case 1:
        if(digitalRead(BUTTON1) == 0){ openTerminal(); Serial.println("Button 1 Pressed - Terminal");}
        if(digitalRead(BUTTON2) == 0){ runAndDebug(); Serial.println("Button 2 Pressed - RunNDebug");}
        if(digitalRead(BUTTON3) == 0){ openGitHub(); Serial.println("Button 3 Pressed - GitHub");}
        if(digitalRead(BUTTON4) == 0){ nextTrack(); Serial.println("Button 4 Pressed - Skip");}

        delay(200);
        if(!dset)
        {
          display.clearDisplay();
          //im2d1 code
          display.drawBitmap(0, 0, im2d2_bmp, 128, 64, WHITE);
          display.display();
          dset = true;
        }
        break;
      default:
        break;
  }

  if(digitalRead(MODE_BUTTON) == 0)
  {
    Serial.println("Profile Switched");
    profile=(profile+1)%2;
    dset = false;
  }

}
